{
  "name": "koa-ssr-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-ts && npm run webpack:production:web && npm run webpack:production:node && npm run build-sass && npm run copy-static-assets",
    "pm2:dev": "pm2 start ecosystem.config.js --watch --env development",
    "pm2:pro": "pm2 start ecosystem.config.js --watch --env production",
    "start": "ts-node app.ts",
    "copy-static-assets": "node copyStaticAssets.js",
    "webpack": "webpack",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run webpack:development:node\" \"npm run webpack:development:web\" \"npm run watch-node\"",
    "watch-node": "nodemon app.ts",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "watch-sass": "node-sass -w static/styles -o static/css",
    "build-sass": "node-sass static/styles -o dist/static/css",
    "webpack:node": "npm run webpack -- --env.presets node --env.presets svg",
    "webpack:web": "npm run webpack -- --env.presets web --env.presets svg",
    "webpack:development:node": "npm run webpack:node -- --env.mode development --env.presets hardsource",
    "webpack:development:web": "npm run webpack:web -- --env.mode development --env.presets hardsource",
    "webpack:production:web": "npm run webpack:web -- --env.mode production",
    "webpack:production:node": "npm run webpack:node -- --env.mode production"
  },
  "dependencies": {
    "@ant-design/icons": "^4.1.0",
    "@babel/polyfill": "^7.8.7",
    "@types/js-cookie": "^2.2.5",
    "@types/koa-logger": "^3.1.1",
    "@types/node": "^13.11.1",
    "@types/react-router-config": "^5.0.1",
    "@types/redux-thunk": "^2.1.0",
    "@types/swiper": "^5.3.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babylonjs": "^4.1.0",
    "concurrently": "^5.2.0",
    "crypto-js": "^4.0.0",
    "css-loader": "^3.5.0",
    "ejs": "^3.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "js-cookie": "^2.2.1",
    "jsesc": "^3.0.0",
    "koa": "^2.11.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.8",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "log4js": "^6.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "moment": "^2.27.0",
    "node-object-hash": "^2.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "protobufjs": "^6.9.0",
    "raf": "^3.4.1",
    "rc-notification": "^4.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "shelljs": "^0.8.4",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "swiper": "^5.3.6",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
